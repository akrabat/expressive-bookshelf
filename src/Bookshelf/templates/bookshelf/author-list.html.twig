{% extends '@layout/default.html.twig' %}


{% set title = 'Authors' %}
{% block title %}{{ title }}{% endblock %}

{% block content %}
<h1>{{ title }}</h1>

<ul>
{% for author in authors %}
    <li><a href="{{ path('author.view', {id: author.id}) }}">{{ author.name }}</a></li>
{% endfor %}
</ul>

{% endblock %}
